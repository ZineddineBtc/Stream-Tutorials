    <% if(!isAuthenticated) { %>
      <%- include("partials/nav-unsigned") %>
    <% } else { %>
      <%- include("partials/nav-signed") %>
    <% } %>
    <input class="form-control" type="text" name="search" id="input-search" placeholder="Search...">
  </div>
</section>
