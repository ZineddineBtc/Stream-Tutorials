    <h1 class="display-4" style="margin-bottom: 3%;" id="h1-my-tutorials">Search Results:</h1>
    <p class="lead" id="p-no-results">Nothing.</p>
    <div class="row row-cols-lg-3 row-cols-md-2 row-cols-1" style="margin-bottom: 2%;">
        <div class="col" id="hidden-col" style="display: none;"></div>
    </div>
  </div>
</div>
